 "linux_64_meta_yaml": {
  "about": {
   "description": "Orange add-on for text mining. It provides access to publicly available\ndata, like NY Times, Twitter and PubMed. Further, it provides tools for\npreprocessing, constructing vector spaces (like bag-of-words and topic\nmodeling) and visualizations like word cloud and geo map. All features\ncan be combined with powerful data mining techniques from the Orange\ndata mining framework.\n",
   "dev_url": "https://github.com/biolab/orange3-text",
   "doc_url": "http://orange3-text.readthedocs.io",
   "home": "https://github.com/biolab/orange3-text",
   "license": "AGPLv3",
   "license_family": "AGPL",
   "license_file": "LICENSE",
   "summary": "Orange3 add-on for analyzing textual data."
  },
  "build": {
   "noarch": "python",
   "number": "0",
   "script": "-m pip install . -vv"
  },
  "extra": {
   "recipe-maintainers": [
    "nikicc",
    "astaric",
    "ajdapretnar",
    "primozgodec"
   ]
  },
  "package": {
   "name": "orange3-text",
   "version": "1.1.0"
  },
  "requirements": {
   "host": [
    "python >=3.6",
    "pip"
   ],
   "run": [
    "python >=3.6",
    "scipy",
    "nltk >=3.0.5,<3.5",
    "scikit-learn",
    "numpy",
    "docutils <0.16",
    "python-dateutil <3.0.0",
    "gensim >=0.12.3",
    "setuptools-git",
    "orange3 >=3.25.0",
    "tweepy",
    "beautifulsoup4",
    "simhash ~=1.9.0",
    "wikipedia",
    "pdfminer3k >=1.3.1",
    "odfpy >=1.3.5",
    "docx2txt >=0.6",
    "lxml",
    "biopython",
    "ufal.udpipe >=1.2.0.3",
    "ply"
   ]
  },
  "source": {
