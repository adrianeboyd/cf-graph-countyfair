   "license_family":"MIT",
   "license_file":"LICENSE",
   "summary":"Python VISA bindings for GPIB, RS232, and USB instruments"
  },
  "build":{
   "noarch":"python",
   "number":"0",
   "script":[
    "-m pip install . -vv"
   ]
  },
